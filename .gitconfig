[alias]
  st = status
  cmm = commit -m
  l  = log
  di = diff
  br = branch
  au = add -u
  ps = push
  or = origin
  pl = pull
  co = checkout
  logg = graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  b  = branch
  bn = !"git rev-parse --abbrev-ref HEAD|tr -d \\\\r\\\\n"
